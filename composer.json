{
    "name": "diverently/livewire-component-resolver",
    "description": "Resolve livewire components from multiple namespaces ",
    "keywords": [
        "laravel",
        "livewire",
        "laravel-livewire",
        "modules",
        "laravel-modules",
        "custom-modules"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Till Schander (wat'n web GmbH)",
            "email": "tillschander@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Diverently\\LivewireComponentResolver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diverently\\LivewireComponentResolver\\LivewireComponentResolverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}